{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#ecffff",
          "foreground": "#4644b9",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#5bff9d",
          "foreground": "#3c0085",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#005f58",
          "foreground": "#ecffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\ueb06",
            "style": "agnoster",
            "max_depth": 3
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
         "background": "#4644b9",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#009a3f{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#009a8c{{ end }}",
            "{{ if gt .Ahead 0 }}#458900{{ end }}",
            "{{ if gt .Behind 0 }}#007098{{ end }}"
          ],

          "foreground": "#e2e3ff",
          "powerline_symbol": "\ue0b0",

          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },

          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "type": "git"
        },
        {
          "background": "#ecffff",
          "foreground": "#42b971",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#ecffff",
          "foreground": "#55b5c0",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ reason .Code }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },

    {
      "alignment": "right",
      "segments": [

        {
          "background": "white",
          "foreground": "green",
          "style": "diamond",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<#7a7a7a> \ue0b3</>",
          "type": "node"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#42b971",
          "style": "diamond",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<#7a7a7a> \ue0b3</>",
          "type": "python"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#3891A6",
          "style": "diamond",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}<#7a7a7a> \ue0b3</>",
          "type": "dotnet"
        },
        {
          "background": "#4636ff",
          "foreground": "#E6E3FF",

          "properties": {
            "time_format": "15:04:05"
          },
          "invert_powerline": true,
          "style": "diamond",
          "powerline_symbol": "\ue0b2",
          "template": " {{ .CurrentDate | date .Format }}<#E6E3FF> \ue641 </>",
          "leading_diamond": "\ue0b2",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "lightYellow",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
